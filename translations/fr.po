msgstr "Choisissez votre fournisseur de compte de messagerie"

#: views/accounts/register/index.hbs:15
msgid "Standard IMAP"
msgstr "IMAP Standard"

#: views/accounts/register/imap-server.hbs:11
msgid "IMAP"
msgstr "IMAP"

#: views/accounts/register/imap-server.hbs:14
#: views/accounts/register/imap-server.hbs:73
msgid "Username"
msgstr "Nom d'utilisateur"

#: views/accounts/register/imap-server.hbs:24
#: views/accounts/register/imap-server.hbs:83
#: views/accounts/register/imap.hbs:31
msgid "Password"
msgstr "Mot de passe"

#: views/accounts/register/imap-server.hbs:34
