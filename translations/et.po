msgid "Choose your email account provider"
msgstr "Valige oma e-posti konto pakkuja"

#: views/accounts/register/index.hbs:15
msgid "Standard IMAP"
msgstr "Standardne IMAP"

#: views/accounts/register/imap-server.hbs:11
msgid "IMAP"
msgstr "IMAP"

#: views/accounts/register/imap-server.hbs:14
#: views/accounts/register/imap-server.hbs:73
msgid "Username"
msgstr "Kasutajanimi"

#: views/accounts/register/imap-server.hbs:24
